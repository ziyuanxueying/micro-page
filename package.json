{
  "name": "@wd/micro-page",
  "version": "0.0.4",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "main": "dist/lib/index.d.ts",
  "module": "dist/lib/index.ts",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "app": "tsc && vite build --outDir dist/app",
    "build": "tsc && vite build",
    "dev": "vite",
    "lib": "tsc && vite build --mode lib",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "prepublish": "pnpm run lib ",
    "postpublish": "pnpm version patch"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.mjs"
  },
  "lint-staged": {
    "*": "prettier --write --cache --ignore-unknown",
    "**/*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.5",
    "dayjs": "^1.11.11",
    "dnd-core": "^16.0.1",
    "immer": "^10.1.1",
    "normalize.css": "^8.0.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-router-dom": "^6.23.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.1",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.11.1",
    "simple-zustand-devtools": "^1.1.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5.3.0",
    "antd": ">=5.16.0",
    "axios": ">=1.6.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "packageManager": "pnpm@9.0.6",
  "publishConfig": {
    "registry": "https://cloudbase-100032138116-npm.pkg.coding.net/repository/npm/"
  }
}
